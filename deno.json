{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "lint": "deno run -A --node-modules-dir=auto npm:eslint .",
    "test": "deno run -A --node-modules-dir=auto npm:vitest",
    "preview": "deno run -A --node-modules-dir=auto npm:vite preview"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@testing-library/dom": "npm:@testing-library/dom@10.4.0",
    "@testing-library/react": "npm:@testing-library/react@16.2.0",
    "jsdom": "npm:jsdom@26.0.0",
    "react": "npm:react@19",
    "react-dom": "npm:react-dom@19.0.0",
    "react-markdown": "npm:react-markdown@10.0.0",
    "remark-gfm": "npm:remark-gfm@4.0.1",
    "terminal.css": "npm:terminal.css@0.7.5",
    "@eslint/js": "npm:@eslint/js@9.19.0",
    "@types/react": "npm:@types/react@19.0.0",
    "@types/react-dom": "npm:@types/react-dom@19.0.0",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@4.3.4",
    "vite": "npm:vite@6.1",
    "vitest": "npm:vitest@3.0.6"
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitOverride": false
  }
}